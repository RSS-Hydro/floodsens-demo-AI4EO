{
	"name": "FloodSENS Python 3.8+",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"settings": {
		"editor.rulers": [100],
		"python.pythonPath": "/usr/bin/python3",
		"python.linting.enabled": true,
		"python.linting.pylintEnabled": true,
		"python.languageServer": "Pylance",
  },
  // Add any extensions you want installed when the container is created
  "extensions": [
    "ms-python.python"
  ]
}